{
  "name": "server",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "dev-win": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" \"src/index.ts\"",
    "deploy": "ts-node src/index.ts",
    "start-zeabur": "ts-node --project ./tsconfig-zeabur.json src/index.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "lint": "eslint 'src/**/*.ts'",
    "lint-win": "eslint \"src/**/*.ts\"",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google-cloud/storage": "^7.15.2",
    "@google-cloud/translate": "^9.0.1",
    "@mui/styled-engine": "^7.0.1",
    "@mui/x-charts": "^7.28.0",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.9",
    "@types/jest": "^29.5.6",
    "@types/mongoose": "^5.10.1",
    "@types/mongoose-autopopulate": "^0.10.1",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "autocannon": "^8.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.19.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.21.0",
    "express": "^4.17.1",
    "haversine-distance": "^1.2.3",
    "jest-mock-extended": "^3.0.5",
    "mongodb-memory-server-core": "^10.1.4",
    "mongoose": "^8.1.1",
    "mongoose-autopopulate": "^1.1.0",
    "nodemon": "^3.0.3",
    "openai": "^4.95.1",
    "pdfkit": "^0.17.0",
    "prettier": "^3.5.3",
    "recharts": "^2.15.1",
    "socket.io": "^4.7.4",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
